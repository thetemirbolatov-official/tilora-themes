    "tokenColors": [
        {
            "name": "Комментарии",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#A0A0CC"
            }
        },
        {
            "name": "Строки",
            "scope": ["string", "punctuation.definition.string"],
            "settings": {
                "foreground": "#00AA77"
            }
        },
        {
            "name": "Ключевые слова",
            "scope": [
                "keyword", "storage", "storage.type",
                "keyword.control", "keyword.operator"
            ],
            "settings": {
                "foreground": "#CC00AA"
            }
        },
        {
            "name": "Функции",
            "scope": [
                "entity.name.function", "support.function",
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#00AADD"
            }
        },
        {
            "name": "Классы и типы",
            "scope": [
                "entity.name.class", "entity.name.type",
                "support.class", "support.type"
            ],
            "settings": {
                "foreground": "#DD9900"
            }
        },
        {
            "name": "Переменные и параметры",
